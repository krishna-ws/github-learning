<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="230.0" prefWidth="230.0" spacing="10.0" style="-fx-border-color: black;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.vm.kioskenhanced2.CardDataController">
   <children>
      <ImageView fx:id="ckImage" fitHeight="54.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/checkin.png" />
         </image>
      </ImageView>
      <Label fx:id="patientName" text="Patient Name" textFill="#140101">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="doctorName" text="Doctor" textFill="#1f0303">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="departmentName" text="Department Name" textFill="#220303">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="appointmentTime" text="Appointment Time" textFill="#160404">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Button fx:id="checkinButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#checkinPage" prefHeight="32.0" prefWidth="83.0" stylesheets="@../styles/style.css" text="Checkin" textFill="WHITE">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
